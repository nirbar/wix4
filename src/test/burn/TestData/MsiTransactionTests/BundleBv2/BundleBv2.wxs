    <PackageGroup Id="BundlePackages">
      <MsiPackage Id="PackageA" SourceFile="$(var.PackageA.TargetPath)" />
      <RollbackBoundary/>
      <MsiTransaction>
          <MsiPackage Id="PackageB" SourceFile="$(var.PackageBv2.TargetPath)" />
